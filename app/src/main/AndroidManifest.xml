<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pablolopezponce.diabetesslm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <service
            android:name="com.pablolopezponce.diabetesslm.ConnectionService"
            />
        <activity
            android:name="com.pablolopezponce.diabetesslm.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pablolopezponce.diabetesslm.WelcomeActivity" >
        </activity>
        <activity android:name="com.pablolopezponce.diabetesslm.AccountSelectActivity" >
        </activity>
        <activity android:name="com.pablolopezponce.diabetesslm.setup.SetupActivity" >
        </activity>
        <activity android:name="com.pablolopezponce.diabetesslm.MainMenuActivity">
        </activity>
    </application>

</manifest>

